<table class="table">
<tr>
<% ["_id", "iid", "sv", "sd", "vtyp", "sno", "sna", "sip", "tot", "sbi", "sarea", "mday", "lat", "lng", "ar", "sareaen", "snaen", "aren", "nbcnt", "bemp", "act"].each do |i| %>
    <th><%= i %></th>
<% end %>
</tr>

<% @ubikes.each do |u| %>
  <tr>
    <% ["_id", "iid", "sv", "sd", "vtyp", "sno", "sna", "sip", "tot", "sbi", "sarea", "mday", "lat", "lng", "ar", "sareaen", "snaen", "aren", "nbcnt", "bemp", "act"].each do |k| %>

      <td><%= u.data[k] %></td>

    <% end %>
  </tr>
<% end %>
</table>